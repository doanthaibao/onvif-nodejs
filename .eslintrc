{
	"env" : {
		"browser": true,
		"node": true
	},

	"globals": {
		"require": true,
		"define": true,
		"console": true,
		"jQuery": true,
		"sap": true,
		"Q": true,
		"$": true,
		"_": true,
		"ace": true,
		"test": true,
		"constants": true,
		"QUnit": true,
		"angular": true
	},
   "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },

	"rules": {
		"curly": 2,
		"no-dupe-keys": 2,
	  	"no-duplicate-case" : 2,
	  	"no-dupe-args" : 2,
		"no-caller": 2,
		"no-constant-condition": 2,
		"no-debugger": 2,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-sparse-arrays": 2,
		"no-undef": 1,
		"no-with": 2,
		"semi": 2,
		"use-isnan": 2,
		"new-parens": 2,
		"no-unreachable": 2,
		"comma-dangle": 2,
		"no-extra-boolean-cast": 2,
		"no-empty": 2,
		"no-extra-semi": 2,
		"no-irregular-whitespace": 2,
		"no-new-func": 2,
		"no-underscore-dangle": 0,
		"no-trailing-spaces": 0,
		"no-mixed-spaces-and-tabs": 0,
		"quotes": 0,
		"dot-notation": 0,
		"new-cap": 0,
		"no-unused-expressions": 0,
		"comma-spacing": 0,
		"no-loop-func": 0,
		"key-spacing": 0,
		"no-use-before-define": 0,
		"space-infix-ops": 0,
		"no-multi-spaces": 0,
		"camelcase": 0,
		"quote-props" : [1, "as-needed", { "keywords": true, "unnecessary": false }],
		"max-params": [1, 3],
        "max-statements": [1, 40],
        "max-depth": [1, 4],
        "max-nested-callbacks": [1, 3]
	}
}
